#################################################################
#    Classes
#################################################################

sri4pv:Photovoltaic
    a owl:Class ;
    rdfs:comment " Photovoltaic is a device that converts sunlight into electrical energy using semiconducting materials. " ;
    rdfs:label "Power Value"@en .


#################################################################
#    Data properties
#################################################################

sri4pv:identifier rdf:type owl:DatatypeProperty ;
                 rdfs:comment "Unique identifier of a device" ;  
                 rdfs:domain sri4pv:Photovoltaic;
                 rdfs:range xsd:string ;
                 rdfs:label "identifier" .


#################################################################
#    Object Properties
#################################################################


sri4all:consistsOfPowerMeasurement rdf:type owl:ObjectProperty ;
                    rdfs:comment "It relates the device with its power measurement " ;  
                    rdfs:domain sri4pv:Photovoltaic ;
                    rdfs:range sri4all:PowerMeasurement  ;
                    rdfs:label "consists of power measurement " .


